{
    "name": "upscale/stdlib-overloading",
    "description": "PHP7 function/method arguments overloading",
    "version": "1.0.3",
    "keywords": ["overload", "overloading", "function", "method", "signature", "arguments"],
    "homepage": "https://github.com/upscalesoftware/stdlib-overloading",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Upscale Software Team",
            "email": "info@upscalesoftware.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": ["src/overload.php"],
        "psr-4": {"Upscale\\Stdlib\\Overloading\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Upscale\\Stdlib\\Overloading\\Tests\\": "tests/"
        }
    }
}
